{
    "configurations": [
        {
            "type": "aws-sam",
            "request": "direct-invoke",
            "invokeTarget": {
                "target": "code",
                "lambdaHandler": "routes.leagues",
                "projectRoot": "${workspaceFolder}/soccerapp/home"
            },
            "lambda": {
